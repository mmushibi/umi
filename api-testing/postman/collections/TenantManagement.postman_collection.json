			"name": "Tenant Creation",
			"item": [
				{
					"name": "Create New Tenant",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 201\", function () {",
									"    pm.response.to.have.status(201);",
									"});",
									"",
									"pm.test(\"Response contains tenant data\", function () {",
									"    const jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('id');",
									"    pm.expect(jsonData).to.have.property('name');",
									"    pm.expect(jsonData).to.have.property('code');",
									"    pm.expect(jsonData).to.have.property('status');",
									"    pm.expect(jsonData.status).to.eql('active');",
									"});",
									"",
									"pm.test(\"Tenant has required properties\", function () {",
									"    const jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('subscriptionPlan');",
									"    pm.expect(jsonData).to.have.property('createdAt');",
									"    pm.expect(jsonData).to.have.property('settings');",
									"});",
									"",
									"// Store tenant data for subsequent tests",
									"if (pm.response.code === 201) {",
									"    const jsonData = pm.response.json();",
									"    pm.environment.set(\"tenant_id\", jsonData.id);",
									"    pm.environment.set(\"tenant_code\", jsonData.code);",
									"    pm.environment.set(\"tenant_name\", jsonData.name);",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
